{
  "name": "retell-backend-node-demo",
  "version": "1.0.0",
  "description": "Example backend written in node. This aims to help people get started connecting their LLM.",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=production nodemon src/index.ts",
    "start": "NODE_ENV=production node dist/index.js",
    "start-dev": "NODE_ENV=development node dist/index.js",
    "build": "npm install && tsc",
    "pm2-start": "NODE_ENV=production pm2 start dist/index.js -i max --name retel-server --watch --ignore-watch=node_modules",
    "pm2-stop": "pm2 stop retel-server",
    "pm2-monitor": "pm2 monit"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/openai": "^1.0.0-beta.11",
    "axios": "^1.6.7",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "cron-parser": "^4.9.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-ws": "^5.0.2",
    "helmet": "^7.1.0",
    "helmet-csp": "^4.0.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.4.1",
    "moment-timezone": "^0.5.45",
    "mongoose": "^8.2.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "openai": "^4.26.0",
    "papaparse": "^5.4.1",
    "pm2": "^5.3.1",
    "redis": "^4.6.14",
    "retell-sdk": "^3.11.1",
    "smee-client": "^2.0.1",
    "twilio": "^4.21.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/eventsource": "^1.1.15",
    "@types/express": "^4.17.21",
    "@types/express-ws": "^3.0.4",
    "@types/hpp": "^0.2.6",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/node-schedule": "^2.1.6",
    "@types/papaparse": "^5.3.14",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "wrangler": "^3.51.0"
  }
